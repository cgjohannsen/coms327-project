#include <stack.h>

int stack_init(stack_t *s)
{
  s->size = 0;
  s->top = NULL;
}

int stack_delete(stack_t *s)
{

}

int stack_peak(stack_t *s, int *value)
{
  if(s->top){
    *value = s->top->value;
    return 0;
  }

  return 1;
}

int stack_push(stack_t *s, int value)
{
  stack_node_t *n;
  if(!(n = malloc(sizeof(*n)))){
    return 1;
  }
  n->value = value;
  n->next = s->top;
  s->top = n;
}
